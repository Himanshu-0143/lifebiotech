# Copy this file to .env when running locally and fill in your values.
# VITE_ prefixed variables are exposed to the frontend by Vite.

# Supabase (public) - safe to expose to the browser
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=your-supabase-anon-key

# Stripe - publishable key for client (exposed), secret key must NOT be exposed and is used by the server
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...   # DO NOT commit your real secret key

# Optional: backend server port for the local Stripe server
STRIPE_SERVER_PORT=4242
